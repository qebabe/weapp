.WSwiper1{
    position: relative;
    max-width: 640px;
    overflow: hidden;
    margin: 0px auto;

}
.weui_grids {
    position: relative;
    overflow: hidden;
}

.weui_grids:before {
    content: '';
    position: absolute;
    box-sizing: border-box;
    width: 200%;
    height: 200%;
    left: 0;
    top: 0;
    border: 1px solid #d9d9d9;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scale(.5);
    transform: scale(.5);
}
.grids-small .grid {
    width: 25%;
    padding: 15px 8px;
}
.grid {
    position: relative;
    float: left;
    padding: 20px 10px;
    width: 33.33333333%;
    box-sizing: border-box;
    text-decoration: none;
}
.weui_grid_icon {
    width: 28px;
    height: 28px;
    margin: 0 auto;
}
.icon {
    font-family: iconfont!important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: .2px;
}
.weui_grid_icon+.weui_grid_label {
    margin-top: 5px;
}

.weui_grid_label {
    display: block;
    text-align: center;
    color: #000;
    font-size: 14px;
}

.side-bar {
	width: 40px;
	position: fixed;
	bottom: 20px;
	right: 0px;
	font-size: 13px;
	line-height: 0;
	z-index: 100;
}
.side-bar .kefu{
	padding-top:10px;
}

