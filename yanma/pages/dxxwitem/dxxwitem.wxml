<import src="../../wxParse/wxParse.wxml"/>
<view class="page">
    <view class="page__hd">
        <view class="page__title">{{itemData.title}}</view>
        <view class="page__desc">文章</view>
    </view>
    <view class="page__bd">
        <view class="weui-article">
            <view class="weui-article__h1">大标题</view>
            <image src="{{item.indexpic.host}}{{item.indexpic.dir}}{{item.indexpic.filepath}}{{item.indexpic.filename}}" style="margin-right: 5px;vertical-align: middle;width:116px; height: 88px;"></image>
            <template is="wxParse" data="{{wxParseData:article.nodes}}"/>


        </view>
    </view>
   
</view>